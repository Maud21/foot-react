{
  "name": "foot-femme-react",
  "version": "1.0.0",
  "description": "MERN stack boilerplate",
  "scripts": {
    "front-end": "npm start --prefix ./front-end",
    "back-end": "nodemon ./back-end/bin/www",
    "app": "concurrently \"npm run front-end\" \"npm run back-end\"",
    "setup": "npm install && cd ./back-end && npm install && cd ../front-end && npm install && cd .."
  },
  "author": "Maud Leal",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "nodemon": "^1.19.3",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2"
  }
}
